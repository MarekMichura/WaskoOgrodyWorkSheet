{
  "cSpell.words": [
    "Archivo",
    "ASPNETCORE",
    "Dima",
    "Eurovia",
    "gifsicle",
    "Henniger",
    "jpegtran",
    "klimanowa",
    "Narama",
    "optipng",
    "Pychowicka",
    "rozhodnik",
    "tseslint",
    "voda",
    "Wasko",
    "webp"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**.sln": true,
    "**/bin": true,
    "**/obj": true,
    "HTML/.babelrc": true,
    "HTML/.dockerignore": true,
    "HTML/.gitignore": true,
    "HTML/.prettierrc": true,
    "HTML/build": true,
    "HTML/node_modules": true,
    "HTML/yarn.lock": true,
    "Server/.dockerignore": true,
    "Server/.gitignore": true,
    "Server/taskCreateDb.sh": true,
    "Server/wait-for-it.sh": true
  },
  "hide-files.files": [
    "HTML/.babelrc",
    "HTML/.dockerignore",
    "HTML/.gitignore",
    "HTML/.prettierrc",
    "HTML/build",
    "HTML/node_modules",
    "HTML/yarn.lock",
    "Server/.dockerignore",
    "Server/.gitignore",
    "Server/taskCreateDb.sh",
    "Server/wait-for-it.sh"
  ]
}
